{
  "manifest_version": 3,
  "name": "Whatsth.is",
  "description": "Website technology and developer toolbox application",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "logo30.png"
  },
  "icons": {
    "128": "logo192.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["logo192.png", "logo30.png"],
      "matches": ["http://*/*", "https://*/*"]
    }
  ],
  "permissions": [
    "tabs"
  ]
}
